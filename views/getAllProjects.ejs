<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Projects</title>
</head>
<body>
    <h1>All Projects</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Description</th>
                <th>Start Date</th>
                <th>End Date</th>
            </tr>
        </thead>
        <tbody>
            <% projects.forEach((project) => { %>
                <tr>
                    <td><%= project.id %></td>
                    <td><%= project.name %></td>
                    <td><%= project.description %></td>
                    <td><%= project.start_date %></td>
                    <td><%= project.end_date %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
